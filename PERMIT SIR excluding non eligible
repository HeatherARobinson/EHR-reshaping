memory.size<-100000
load("crea.repv13.rda")
load("sir.datahfonly.Rdata")
sir.data<-sir.data[sir.data$ReadCode=="44J3.",]
sir.data<-sir.data[sir.data$PatientID %in% crea.rep$PatientID,]

prac<-read.csv("PractiseKeySIR.csv")
pats<-merge(sir.data[,c(1,4,7)],prac,all.x=TRUE)
pats<-pats[order(pats$Source),]
pats<-pats[unique(pats[,c(1,2)]),]
